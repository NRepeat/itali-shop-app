datasource db {
  provider = "mysql" // Example: This external one is MySQL
  url      = env("DATABASE_URL_EXTERNAL")
  // Generate the client into a different specific folder
}

generator client {
  provider      = "prisma-client"
  output        = "./generated/external_client"
  moduleFormat  = "esm"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x", "linux-musl-openssl-3.0.x"]
}

